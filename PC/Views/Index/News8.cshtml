<script>
    $(document).ready(function () {
        $('#btnSubmit').click(function () {
            if (checkInput() == true) {
                pageAjax();
            }
            else {
                return false;
            }
        });


    });
    function pageAjax() {
        $.ajax({
            type: "post",
            url: "/CustomerRequestManage/CustomerRequest/CustomerRequestEdit",
            data: $('form').serialize(),
            dataType: "json",
            async: false,
            success: function (response) { 
                if(response.Status == 1){
                    alert("提交成功！我们会在3个工作日内与您联系哦~");
                }
                else{
                    alert('申请失败');
                }
            },
            error: function (response){ }
        });

    }

    function checkInput() {
        var customerName = $('#txtCustomerName').val();
        var contacts = $('#txtContacts').val();
        var position = $('#txtPosition').val();
        var phoneNumber = $('#txtPhoneNumber').val();
        var qqNumber = $('#txtQQNumber').val();

        if (customerName == '' || lTrim(customerName) == '') {
            alert('请输入公司/店铺名称');
            return false;
        }
        if (customerName.length > 25) {
            alert('公司/店铺名称不能超过 25字');
            return false;
        }

        if (contacts == '' || lTrim(contacts) == '') {
            alert('请输入联系人');
            return false;
        }
        if (contacts.length > 25) {
            alert('联系人不能超过 25字');
            return false;
        }

        if (position == '' || lTrim(position) == '') {
            alert('请输入职位');
            return false;
        }
        if (position.length > 10) {
            alert('职位不能超过 10字');
            return false;
        }

        if (phoneNumber == '' || lTrim(phoneNumber) == '') {
            alert('请输入手机号码');
            return false;
        }
        if (isPhoneNumber(phoneNumber) == false) {
            alert('手机号码输入不正确');
            return false;
        }

        if (qqNumber == '' || lTrim(qqNumber) == '') {
            alert('请输入QQ号码');
            return false;
        }
        if (isQQNumber(qqNumber) == false) {
            alert('QQ号码输入不正确（非零开头的四位以上的数字）');
            return false;
        }
        if (qqNumber.length > 20) {
            alert('QQ号码不能超过 20位');
            return false;
        }
        return true;
    }

    //去除左侧空格
    function lTrim(str) {
        return str.replace(/(^\s*)/g, "");
    }
    //是否为手机号码
    function isPhoneNumber(number) {
        var reg = /^[1]{1}\d{10}$/;
        return reg.test(number);
    }

    function isQQNumber(number) {
        var reg = /^[1-9][0-9]{4,}$/;
        return reg.test(number);
    }
   
</script>